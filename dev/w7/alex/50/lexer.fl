%class-name = "Scanner"
%filenames = "scanner"
%interactive

integral            [0-9]+
floating            {integral}\.{integral}
number              {integral}|{floating}
operation           "$"
ws                  [ \t\r\n]+

%%

{number}            return Tokens::NUMBER;
{operation}         return Tokens::OPERATION;
{ws}
.                   return matched()[0];
