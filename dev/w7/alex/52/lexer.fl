%class-name = "Scanner"
%filenames = "scanner"
%interactive

integral            [0-9]+
floating            {integral}\.{integral}
number              {integral}|{floating}
ws                  [ \t\n\r]+

%%

{number}            return Tokens::NR;
{ws}
"!="                return Tokens::INEQ;
.                   return matched()[0];
