%interactive
integral          [[:digit:]]+
double1           -?[[:digit:]]+\.?[[:digit:]]*
double2           -?\.[[:digit:]]+
double            {double1}|{double2}

%%
{integral}                      return Tokens::INTEGRAL;
{double}                        return Tokens::DOUBLE;
([[:space:]]{-}[\n])+           // Ignore irrelevant whitespace
(.|\n)                          return matched()[0];
